<%= form_with model:@user,url:users_path,method: :post do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :body %>
  <%= f.text_area :body %>
  <%= f.label :image %>
  <%= f.file_field :image,accept:"image/*" %>
  <%= f.submit "投稿" %>
<% end %>

<% @users.each do |user| %>
  <%= user.title %>
  <%= user.body %>
  <%= image_tag user.get_image(100,100) %></br >
<% end %>